in\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      if (isAuthenticated \u003d\u003d\u003d \u0027true\u0027) {\n        window.location.href \u003d \u0027dashboard.html\u0027;\n      }\n      \n      // Handle login form submission\n      document.getElementById(\u0027loginForm\u0027).addEventListener(\u0027submit\u0027, function(e) {\n        e.preventDefault();\n        \n        const email \u003d document.getElementById(\u0027email\u0027).value;\n        const password \u003d document.getElementById(\u0027password\u0027).value;\n        const errorMessage \u003d document.getElementById(\u0027errorMessage\u0027);\n        const loginButton \u003d document.getElementById(\u0027loginButton\u0027);\n        \n        // Reset error message\n        errorMessage.style.display \u003d \u0027none\u0027;\n        \n        // Show loading state\n        loginButton.innerHTML \u003d \u0027\u003cspan class\u003d\"spinner-border spinner-border-sm\" role\u003d\"status\" aria-hidden\u003d\"true\"\u003e\u003c/span\u003e Loading...\u0027;\n        loginButton.disabled \u003d true;\n        \n        // Simulate API call (in a real app, this would call your backend)\n        setTimeout(() \u003d\u003e {\n          // For demo purposes, accept any credentials\n          localStorage.setItem(\u0027isAuthenticated\u0027, \u0027true\u0027);\n          localStorage.setItem(\u0027userEmail\u0027, email);\n          \n          // Redirect to dashboard\n          window.location.href \u003d \u0027dashboard.html\u0027;\n        }, 1500);\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/signup.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Sign Up\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .auth-container {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      min-height: calc(100vh - 200px);\n    }\n    .auth-card {\n      background-color: white;\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);\n      width: 100%;\n      max-width: 450px;\n      overflow: hidden;\n    }\n    .auth-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      padding: 20px;\n      text-align: center;\n    }\n    .auth-body {\n      padding: 30px;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 20px;\n      border: 1px solid #ddd;\n    }\n    .form-control:focus {\n      border-color: #8E54E9;\n      box-shadow: 0 0 0 0.2rem rgba(142, 84, 233, 0.25);\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 12px;\n      font-weight: bold;\n      width: 100%;\n      margin-top: 10px;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .auth-footer {\n      text-align: center;\n      margin-top: 20px;\n      color: #666;\n    }\n    .auth-footer a {\n      color: #4776E6;\n      text-decoration: none;\n    }\n    .auth-footer a:hover {\n      text-decoration: underline;\n    }\n    .error-message {\n      color: #dc3545;\n      background-color: #f8d7da;\n      border: 1px solid #f5c6cb;\n      border-radius: 8px;\n      padding: 10px;\n      margin-bottom: 20px;\n      display: none;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"login.html\"\u003eLogin\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link active\" href\u003d\"signup.html\"\u003eSign Up\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"auth-container\"\u003e\n      \u003cdiv class\u003d\"auth-card\"\u003e\n        \u003cdiv class\u003d\"auth-header\"\u003e\n          \u003ch2\u003eSign Up\u003c/h2\u003e\n          \u003cp\u003eCreate your scientific workspace\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"auth-body\"\u003e\n          \u003cdiv id\u003d\"errorMessage\" class\u003d\"error-message\"\u003e\u003c/div\u003e\n          \u003cform id\u003d\"signupForm\"\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"email\" class\u003d\"form-label\"\u003eEmail address\u003c/label\u003e\n              \u003cinput type\u003d\"email\" class\u003d\"form-control\" id\u003d\"email\" placeholder\u003d\"name@example.com\" required\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"password\" class\u003d\"form-label\"\u003ePassword\u003c/label\u003e\n              \u003cinput type\u003d\"password\" class\u003d\"form-control\" id\u003d\"password\" placeholder\u003d\"Create a password\" required\u003e\n              \u003cdiv class\u003d\"form-text\"\u003ePassword must be at least 8 characters long.\u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"confirmPassword\" class\u003d\"form-label\"\u003eConfirm Password\u003c/label\u003e\n              \u003cinput type\u003d\"password\" class\u003d\"form-control\" id\u003d\"confirmPassword\" placeholder\u003d\"Confirm your password\" required\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"submit\" class\u003d\"btn btn-primary\" id\u003d\"signupButton\"\u003eSign Up\u003c/button\u003e\n          \u003c/form\u003e\n          \u003cdiv class\u003d\"auth-footer\"\u003e\n            \u003cp\u003eAlready have an account? \u003ca href\u003d\"login.html\"\u003eLogin\u003c/a\u003e\u003c/p\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is already logged in\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      if (isAuthenticated \u003d\u003d\u003d \u0027true\u0027) {\n        window.location.href \u003d \u0027dashboard.html\u0027;\n      }\n      \n      // Handle signup form submission\n      document.getElementById(\u0027signupForm\u0027).addEventListener(\u0027submit\u0027, function(e) {\n        e.preventDefault();\n        \n        const email \u003d document.getElementById(\u0027email\u0027).value;\n        const password \u003d document.getElementById(\u0027password\u0027).value;\n        const confirmPassword \u003d document.getElementById(\u0027confirmPassword\u0027).value;\n        const errorMessage \u003d document.getElementById(\u0027errorMessage\u0027);\n        const signupButton \u003d document.getElementById(\u0027signupButton\u0027);\n        \n        // Reset error message\n        errorMessage.style.display \u003d \u0027none\u0027;\n        \n        // Validate passwords match\n        if (password !\u003d\u003d confirmPassword) {\n          errorMessage.textContent \u003d \u0027Passwords do not match\u0027;\n          errorMessage.style.display \u003d \u0027block\u0027;\n          return;\n        }\n        \n        // Validate password strength\n        if (password.length \u003c 8) {\n          errorMessage.textContent \u003d \u0027Password must be at least 8 characters long\u0027;\n          errorMessage.style.display \u003d \u0027block\u0027;\n          return;\n        }\n        \n        // Show loading state\n        signupButton.innerHTML \u003d \u0027\u003cspan class\u003d\"spinner-border spinner-border-sm\" role\u003d\"status\" aria-hidden\u003d\"true\"\u003e\u003c/span\u003e Loading...\u0027;\n        signupButton.disabled \u003d true;\n        \n        // Simulate API call (in a real app, this would call your backend)\n        setTimeout(() \u003d\u003e {\n          // For demo purposes, accept any valid signup\n          signupButton.innerHTML \u003d \u0027Success!\u0027;\n          \n          // Redirect to login page after a short delay\n          setTimeout(() \u003d\u003e {\n            window.location.href \u003d \u0027login.html\u0027;\n          }, 1000);\n        }, 1500);\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/dashboard.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Dashboard\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .dashboard-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n      transition: transform 0.3s ease;\n    }\n    .card:hover {\n      transform: translateY(-5px);\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .btn-outline-primary {\n      color: #4776E6;\n      border-color: #4776E6;\n      border-radius: 8px;\n      padding: 10px 20px;\n    }\n    .btn-outline-primary:hover {\n      background-color: #4776E6;\n      color: white;\n    }\n    .credits-badge {\n      background: linear-gradient(to right, #FF512F, #F09819);\n      color: white;\n      font-weight: bold;\n      padding: 10px 20px;\n      border-radius: 50px;\n      display: inline-block;\n    }\n    .experiment-card {\n      height: 100%;\n    }\n    .experiment-status {\n      display: inline-block;\n      padding: 5px 10px;\n      border-radius: 50px;\n      font-size: 12px;\n      font-weight: bold;\n    }\n    .status-completed {\n      background-color: #28a745;\n      color: white;\n    }\n    .status-progress {\n      background-color: #007bff;\n      color: white;\n    }\n    .user-welcome {\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link active\" href\u003d\"dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"dashboard-header\"\u003e\n      \u003cdiv class\u003d\"user-welcome\"\u003e\n        \u003cdiv\u003e\n          \u003ch1\u003eDashboard\u003c/h1\u003e\n          \u003cp class\u003d\"lead\"\u003eWelcome to your scientific workspace\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"credits-badge\"\u003e\n          Credits: \u003cspan id\u003d\"creditsCount\"\u003e3\u003c/span\u003e remaining\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row mb-4\"\u003e\n      \u003cdiv class\u003d\"col-12\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between align-items-center\"\u003e\n              \u003ch2 class\u003d\"mb-0\"\u003eMy Experiments\u003c/h2\u003e\n              \u003cbutton class\u003d\"btn btn-primary\" id\u003d\"newExperimentBtn\"\u003e\n                \u003ci class\u003d\"bi bi-plus-circle me-2\"\u003e\u003c/i\u003e New Experiment\n              \u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\" id\u003d\"experimentsContainer\"\u003e\n      \u003c!-- Experiments will be loaded here --\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027login.html\u0027;\n      });\n      \n      // Handle new experiment button\n      document.getElementById(\u0027newExperimentBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would create a new experiment and redirect to the workflow\n        const experimentId \u003d \u0027exp_\u0027 + Math.random().toString(36).substr(2, 9);\n        const title \u003d \u0027New Experiment\u0027;\n        const status \u003d \u0027In Progress\u0027;\n        const lastUpdated \u003d new Date().toISOString();\n        const currentStep \u003d \u0027Question Formulation\u0027;\n        \n        createExperimentCard(experimentId, title, status, lastUpdated, currentStep);\n      });\n      \n      // Load sample experiments\n      loadSampleExperiments();\n    });\n    \n    function loadSampleExperiments() {\n      const sampleExperiments \u003d [\n        {\n          id: \u0027exp_123456789\u0027,\n          title: \u0027Effect of Temperature on Reaction Rates\u0027,\n          status: \u0027In Progress\u0027,\n          lastUpdated: \u00272025-05-20T14:30:00Z\u0027,\n          currentStep: \u0027Methodology Design\u0027\n        },\n        {\n          id: \u0027exp_987654321\u0027,\n          title: \u0027Plant Growth Under Different Light Conditions\u0027,\n          status: \u0027Completed\u0027,\n          lastUpdated: \u00272025-05-15T09:45:00Z\u0027,\n          currentStep: \u0027Published\u0027\n        }\n      ];\n      \n      sampleExperiments.forEach(exp \u003d\u003e {\n        createExperimentCard(exp.id, exp.title, exp.status, exp.lastUpdated, exp.currentStep);\n      });\n    }\n    \n    function createExperimentCard(id, title, status, lastUpdated, currentStep) {\n      const container \u003d document.getElementById(\u0027experimentsContainer\u0027);\n      const date \u003d new Date(lastUpdated);\n      const formattedDate \u003d date.toLocaleDateString() + \u0027 \u0027 + date.toLocaleTimeString();\n      \n      const card \u003d document.createElement(\u0027div\u0027);\n      card.className \u003d \u0027col-md-6 col-lg-4 mb-4\u0027;\n      card.innerHTML \u003d `\n        \u003cdiv class\u003d\"card experiment-card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003e${title}\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cp\u003e\n              \u003cspan class\u003d\"experiment-status ${status \u003d\u003d\u003d \u0027Completed\u0027 ? \u0027status-completed\u0027 : \u0027status-progress\u0027}\"\u003e${status}\u003c/span\u003e\n              ${currentStep ? `\u003csmall class\u003d\"text-muted ms-2\"\u003eCurrent step: ${currentStep}\u003c/small\u003e` : \u0027\u0027}\n            \u003c/p\u003e\n            \u003cp class\u003d\"text-muted small\"\u003eLast updated: ${formattedDate}\u003c/p\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-3\"\u003e\n              \u003cbutton class\u003d\"btn btn-outline-primary btn-sm view-details\" data-id\u003d\"${id}\"\u003eView Details\u003c/button\u003e\n              \u003cbutton class\u003d\"btn btn-primary btn-sm continue-experiment\" data-id\u003d\"${id}\" data-step\u003d\"${currentStep}\"\u003eContinue\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      `;\n      \n      container.prepend(card);\n      \n      // Add event listeners to the newly created buttons\n      const continueBtn \u003d card.querySelector(\u0027.continue-experiment\u0027);\n      continueBtn.addEventListener(\u0027click\u0027, function() {\n        const step \u003d this.getAttribute(\u0027data-step\u0027);\n        navigateToWorkflowStep(step);\n      });\n      \n      const viewDetailsBtn \u003d card.querySelector(\u0027.view-details\u0027);\n      viewDetailsBtn.addEventListener(\u0027click\u0027, function() {\n        const experimentId \u003d this.getAttribute(\u0027data-id\u0027);\n        alert(`Viewing details for experiment ${experimentId}`);\n        // In a real app, this would navigate to a detailed view of the experiment\n      });\n    }\n    \n    function navigateToWorkflowStep(step) {\n      // Map the current step to the corresponding workflow page\n      const stepToPageMap \u003d {\n        \u0027Question Formulation\u0027: \u0027workflow/question-formulation.html\u0027,\n        \u0027Research Explorer\u0027: \u0027workflow/research-explorer.html\u0027,\n        \u0027Hypothesis Builder\u0027: \u0027workflow/hypothesis-builder.html\u0027,\n        \u0027Methodology Design\u0027: \u0027workflow/methodology-designer.html\u0027,\n        \u0027Data Gatherer\u0027: \u0027workflow/data-gatherer.html\u0027,\n        \u0027Experiment Runner\u0027: \u0027workflow/experiment-runner.html\u0027,\n        \u0027Data Analyzer\u0027: \u0027workflow/data-analyzer.html\u0027,\n        \u0027Conclusion Drawer\u0027: \u0027workflow/conclusion-drawer.html\u0027,\n        \u0027Peer Reviewer\u0027: \u0027workflow/peer-reviewer.html\u0027,\n        \u0027Published\u0027: \u0027workflow/publication-exporter.html\u0027\n      };\n      \n      const page \u003d stepToPageMap[step] || \u0027workflow/question-formulation.html\u0027;\n      window.location.href \u003d page;\n    }\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/question-formulation.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Question Formulation\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .ai-suggestion {\n      background-color: rgba(255, 255, 255, 0.1);\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      color: white;\n    }\n    .ai-suggestion-header {\n      display: flex;\n      align-items: center;\n      margin-bottom: 10px;\n    }\n    .ai-icon {\n      width: 30px;\n      height: 30px;\n      background-color: #F09819;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      margin-right: 10px;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eQuestion Formulation\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 1 of 10: Define your research question\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 10%\" aria-valuenow\u003d\"10\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step active\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eDefine Your Research Question\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cform id\u003d\"questionForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"experimentTitle\" class\u003d\"form-label\"\u003eExperiment Title\u003c/label\u003e\n                \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"experimentTitle\" placeholder\u003d\"Enter a descriptive title for your experiment\"\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"researchQuestion\" class\u003d\"form-label\"\u003eResearch Question\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"researchQuestion\" rows\u003d\"3\" placeholder\u003d\"What specific question are you trying to answer?\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"researchDomain\" class\u003d\"form-label\"\u003eResearch Domain\u003c/label\u003e\n                \u003cselect class\u003d\"form-select\" id\u003d\"researchDomain\"\u003e\n                  \u003coption selected\u003eSelect a domain\u003c/option\u003e\n                  \u003coption value\u003d\"biology\"\u003eBiology\u003c/option\u003e\n                  \u003coption value\u003d\"chemistry\"\u003eChemistry\u003c/option\u003e\n                  \u003coption value\u003d\"physics\"\u003ePhysics\u003c/option\u003e\n                  \u003coption value\u003d\"psychology\"\u003ePsychology\u003c/option\u003e\n                  \u003coption value\u003d\"sociology\"\u003eSociology\u003c/option\u003e\n                  \u003coption value\u003d\"computer_science\"\u003eComputer Science\u003c/option\u003e\n                  \u003coption value\u003d\"other\"\u003eOther\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"keywords\" class\u003d\"form-label\"\u003eKeywords\u003c/label\u003e\n                \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"keywords\" placeholder\u003d\"Enter keywords separated by commas\"\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"d-flex justify-content-between\"\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"generateAiSuggestions\"\u003eGenerate AI Suggestions\u003c/button\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Research Explorer\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAI Suggestions\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eResearch Question Refinement\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eConsider narrowing your question to focus on specific variables that can be measured.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eExample Questions\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cul\u003e\n                \u003cli\u003eHow does temperature affect the rate of photosynthesis in aquatic plants?\u003c/li\u003e\n                \u003cli\u003eWhat is the relationship between sleep duration and cognitive performance in college students?\u003c/li\u003e\n                \u003cli\u003eHow does the concentration of salt affect the freezing point of water?\u003c/li\u003e\n              \u003c/ul\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Save form data to localStorage\n        const experimentTitle \u003d document.getElementById(\u0027experimentTitle\u0027).value;\n        const researchQuestion \u003d document.getElementById(\u0027researchQuestion\u0027).value;\n        const researchDomain \u003d document.getElementById(\u0027researchDomain\u0027).value;\n        const keywords \u003d document.getElementById(\u0027keywords\u0027).value;\n        \n        localStorage.setItem(\u0027experimentTitle\u0027, experimentTitle);\n        localStorage.setItem(\u0027researchQuestion\u0027, researchQuestion);\n        localStorage.setItem(\u0027researchDomain\u0027, researchDomain);\n        localStorage.setItem(\u0027keywords\u0027, keywords);\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Research Explorer\u0027);\n        \n        // Navigate to next step\n        window.location.href \u003d \u0027research-explorer.html\u0027;\n      });\n      \n      // Handle AI suggestions button\n      document.getElementById(\u0027generateAiSuggestions\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call the LLM API\n        alert(\u0027AI suggestions would be generated based on your input.\u0027);\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/research-explorer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Research Explorer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .search-result {\n      background-color: white;\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      transition: transform 0.3s ease;\n    }\n    .search-result:hover {\n      transform: translateY(-5px);\n      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n    }\n    .search-result h5 {\n      color: #4776E6;\n      margin-bottom: 10px;\n    }\n    .search-result p {\n      color: #666;\n      margin-bottom: 10px;\n    }\n    .search-result .source {\n      color: #999;\n      font-size: 12px;\n    }\n    .search-result .badge {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      margin-right: 5px;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eResearch Explorer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 2 of 10: Explore existing research\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 20%\" aria-valuenow\u003d\"20\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eSearch Parameters\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cform id\u003d\"searchForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"searchQuery\" class\u003d\"form-label\"\u003eSearch Query\u003c/label\u003e\n                \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"searchQuery\" placeholder\u003d\"Enter keywords to search\"\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"searchDomain\" class\u003d\"form-label\"\u003eDomain\u003c/label\u003e\n                \u003cselect class\u003d\"form-select\" id\u003d\"searchDomain\"\u003e\n                  \u003coption selected\u003eAll Domains\u003c/option\u003e\n                  \u003coption value\u003d\"biology\"\u003eBiology\u003c/option\u003e\n                  \u003coption value\u003d\"chemistry\"\u003eChemistry\u003c/option\u003e\n                  \u003coption value\u003d\"physics\"\u003ePhysics\u003c/option\u003e\n                  \u003coption value\u003d\"psychology\"\u003ePsychology\u003c/option\u003e\n                  \u003coption value\u003d\"sociology\"\u003eSociology\u003c/option\u003e\n                  \u003coption value\u003d\"computer_science\"\u003eComputer Science\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"searchYear\" class\u003d\"form-label\"\u003ePublication Year\u003c/label\u003e\n                \u003cselect class\u003d\"form-select\" id\u003d\"searchYear\"\u003e\n                  \u003coption selected\u003eAll Years\u003c/option\u003e\n                  \u003coption value\u003d\"2025\"\u003e2025\u003c/option\u003e\n                  \u003coption value\u003d\"2024\"\u003e2024\u003c/option\u003e\n                  \u003coption value\u003d\"2023\"\u003e2023\u003c/option\u003e\n                  \u003coption value\u003d\"2022\"\u003e2022\u003c/option\u003e\n                  \u003coption value\u003d\"2021\"\u003e2021\u003c/option\u003e\n                  \u003coption value\u003d\"2020\"\u003e2020\u003c/option\u003e\n                  \u003coption value\u003d\"older\"\u003eBefore 2020\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary w-100\" id\u003d\"searchBtn\"\u003eSearch\u003c/button\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"card mt-4\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eSaved References\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv id\u003d\"savedReferences\"\u003e\n              \u003cp class\u003d\"text-muted\"\u003eNo references saved yet.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-3\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Hypothesis Builder\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eSearch Results\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv id\u003d\"searchResults\"\u003e\n              \u003c!-- Sample search results --\u003e\n              \u003cdiv class\u003d\"search-result\"\u003e\n                \u003ch5\u003eThe Effect of Temperature on Chemical Reaction Rates: A Comprehensive Review\u003c/h5\u003e\n                \u003cp\u003eThis review examines the relationship between temperature and reaction rates across various chemical processes, providing insights into the Arrhenius equation and its applications.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eChemistry\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eThermodynamics\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eKinetics\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"source\"\u003eJournal of Chemical Kinetics, 2024 | Authors: J. Smith, A. Johnson\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary save-reference\"\u003eSave Reference\u003c/button\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"search-result\"\u003e\n                \u003ch5\u003eExperimental Methods for Measuring Reaction Rates at Various Temperatures\u003c/h5\u003e\n                \u003cp\u003eThis paper presents novel experimental methods for accurately measuring reaction rates across a wide range of temperatures, with applications in industrial chemistry and biochemistry.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eMethodology\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eExperimental Design\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eChemistry\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"source\"\u003eAnalytical Chemistry, 2023 | Authors: L. Zhang, M. Patel\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary save-reference\"\u003eSave Reference\u003c/button\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"search-result\"\u003e\n                \u003ch5\u003eMathematical Modeling of Temperature-Dependent Reaction Kinetics\u003c/h5\u003e\n                \u003cp\u003eThis study develops advanced mathematical models for predicting reaction rates based on temperature variations, with validation across multiple chemical systems.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eMathematical Modeling\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eComputational Chemistry\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eKinetics\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"source\"\u003eJournal of Computational Chemistry, 2022 | Authors: R. Brown, S. Davis\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary save-reference\"\u003eSave Reference\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle search button\n      document.getElementById(\u0027searchBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call an API to search for papers\n        alert(\u0027Search functionality would be implemented with real data in the production version.\u0027);\n      });\n      \n      // Handle save reference buttons\n      const saveButtons \u003d document.querySelectorAll(\u0027.save-reference\u0027);\n      saveButtons.forEach(button \u003d\u003e {\n        button.addEventListener(\u0027click\u0027, function() {\n          const result \u003d this.closest(\u0027.search-result\u0027);\n          const title \u003d result.querySelector(\u0027h5\u0027).textContent;\n          \n          // Add to saved references\n          const savedReferences \u003d document.getElementById(\u0027savedReferences\u0027);\n          if (savedReferences.querySelector(\u0027.text-muted\u0027)) {\n            savedReferences.innerHTML \u003d \u0027\u0027;\n          }\n          \n          const referenceItem \u003d document.createElement(\u0027div\u0027);\n          referenceItem.className \u003d \u0027mb-2\u0027;\n          referenceItem.innerHTML \u003d `\n            \u003cp class\u003d\"mb-1\"\u003e\u003cstrong\u003e${title}\u003c/strong\u003e\u003c/p\u003e\n            \u003csmall class\u003d\"text-muted\"\u003e${result.querySelector(\u0027.source\u0027).textContent}\u003c/small\u003e\n          `;\n          \n          savedReferences.appendChild(referenceItem);\n          \n          // Disable the button\n          this.disabled \u003d true;\n          this.textContent \u003d \u0027Saved\u0027;\n          this.classList.remove(\u0027btn-outline-primary\u0027);\n          this.classList.add(\u0027btn-success\u0027);\n        });\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027question-formulation.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Hypothesis Builder\u0027);\n        window.location.href \u003d \u0027hypothesis-builder.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/hypothesis-builder.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Hypothesis Builder\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .ai-suggestion {\n      background-color: rgba(255, 255, 255, 0.1);\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      color: white;\n    }\n    .ai-suggestion-header {\n      display: flex;\n      align-items: center;\n      margin-bottom: 10px;\n    }\n    .ai-icon {\n      width: 30px;\n      height: 30px;\n      background-color: #F09819;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      margin-right: 10px;\n    }\n    .hypothesis-quality {\n      display: flex;\n      align-items: center;\n      margin-top: 20px;\n    }\n    .quality-meter {\n      flex-grow: 1;\n      height: 10px;\n      background-color: #e9ecef;\n      border-radius: 5px;\n      margin: 0 10px;\n      overflow: hidden;\n    }\n    .quality-fill {\n      height: 100%;\n      background: linear-gradient(to right, #FF512F, #F09819);\n      width: 75%;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eHypothesis Builder\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 3 of 10: Formulate your hypothesis\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 30%\" aria-valuenow\u003d\"30\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eFormulate Your Hypothesis\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cform id\u003d\"hypothesisForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"hypothesisType\" class\u003d\"form-label\"\u003eHypothesis Type\u003c/label\u003e\n                \u003cselect class\u003d\"form-select\" id\u003d\"hypothesisType\"\u003e\n                  \u003coption selected\u003eSelect hypothesis type\u003c/option\u003e\n                  \u003coption value\u003d\"directional\"\u003eDirectional Hypothesis\u003c/option\u003e\n                  \u003coption value\u003d\"non_directional\"\u003eNon-Directional Hypothesis\u003c/option\u003e\n                  \u003coption value\u003d\"null\"\u003eNull Hypothesis\u003c/option\u003e\n                  \u003coption value\u003d\"alternative\"\u003eAlternative Hypothesis\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"hypothesisStatement\" class\u003d\"form-label\"\u003eHypothesis Statement\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"hypothesisStatement\" rows\u003d\"4\" placeholder\u003d\"If [independent variable], then [dependent variable] will [expected outcome]\"\u003e\u003c/textarea\u003e\n                \u003csmall class\u003d\"text-muted\"\u003eFormulate your hypothesis as a clear, testable statement that predicts the relationship between variables.\u003c/small\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"nullHypothesis\" class\u003d\"form-label\"\u003eNull Hypothesis (H₀)\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"nullHypothesis\" rows\u003d\"2\" placeholder\u003d\"There is no relationship between [independent variable] and [dependent variable]\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"alternativeHypothesis\" class\u003d\"form-label\"\u003eAlternative Hypothesis (H₁)\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"alternativeHypothesis\" rows\u003d\"2\" placeholder\u003d\"There is a relationship between [independent variable] and [dependent variable]\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"justification\" class\u003d\"form-label\"\u003eJustification\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"justification\" rows\u003d\"3\" placeholder\u003d\"Explain why you believe this hypothesis is valid based on your research\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"hypothesis-quality\"\u003e\n                \u003cspan\u003eHypothesis Quality:\u003c/span\u003e\n                \u003cdiv class\u003d\"quality-meter\"\u003e\n                  \u003cdiv class\u003d\"quality-fill\"\u003e\u003c/div\u003e\n                \u003c/div\u003e\n                \u003cspan\u003e75%\u003c/span\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Methodology Designer\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAI Suggestions\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eHypothesis Structure\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eA strong hypothesis should be clear, specific, and testable. It should identify the independent and dependent variables and predict their relationship.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eSuggested Hypothesis\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eBased on your research question and literature review, consider: \"If the temperature of the reaction environment increases, then the rate of the chemical reaction will increase proportionally.\"\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eImprovement Tips\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cul\u003e\n                \u003cli\u003eSpecify the temperature range\u003c/li\u003e\n                \u003cli\u003eDefine how reaction rate will be measured\u003c/li\u003e\n                \u003cli\u003eConsider adding a quantitative prediction\u003c/li\u003e\n              \u003c/ul\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary w-100 mt-3\" id\u003d\"refineHypothesis\"\u003eRefine My Hypothesis\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027research-explorer.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Save form data to localStorage\n        const hypothesisType \u003d document.getElementById(\u0027hypothesisType\u0027).value;\n        const hypothesisStatement \u003d document.getElementById(\u0027hypothesisStatement\u0027).value;\n        const nullHypothesis \u003d document.getElementById(\u0027nullHypothesis\u0027).value;\n        const alternativeHypothesis \u003d document.getElementById(\u0027alternativeHypothesis\u0027).value;\n        const justification \u003d document.getElementById(\u0027justification\u0027).value;\n        \n        localStorage.setItem(\u0027hypothesisType\u0027, hypothesisType);\n        localStorage.setItem(\u0027hypothesisStatement\u0027, hypothesisStatement);\n        localStorage.setItem(\u0027nullHypothesis\u0027, nullHypothesis);\n        localStorage.setItem(\u0027alternativeHypothesis\u0027, alternativeHypothesis);\n        localStorage.setItem(\u0027justification\u0027, justification);\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Methodology Design\u0027);\n        \n        // Navigate to next step\n        window.location.href \u003d \u0027methodology-designer.html\u0027;\n      });\n      \n      // Handle refine hypothesis button\n      document.getElementById(\u0027refineHypothesis\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call the LLM API\n        alert(\u0027AI would refine your hypothesis based on your input and research.\u0027);\n        \n        // For demo purposes, fill in some sample data\n        document.getElementById(\u0027hypothesisType\u0027).value \u003d \u0027directional\u0027;\n        document.getElementById(\u0027hypothesisStatement\u0027).value \u003d \u0027If the temperature of the reaction environment increases from 10°C to 50°C, then the rate of the chemical reaction will increase according to the Arrhenius equation, with approximately a doubling of rate for each 10°C increase.\u0027;\n        document.getElementById(\u0027nullHypothesis\u0027).value \u003d \u0027There is no relationship between temperature and the rate of the chemical reaction.\u0027;\n        document.getElementById(\u0027alternativeHypothesis\u0027).value \u003d \u0027There is a positive relationship between temperature and the rate of the chemical reaction.\u0027;\n        document.getElementById(\u0027justification\u0027).value \u003d \u0027According to the Arrhenius equation and the collision theory, higher temperatures provide more kinetic energy to molecules, increasing the frequency of collisions and the proportion of collisions with sufficient energy to overcome the activation energy barrier. This is supported by multiple studies in the literature review.\u0027;\n        \n        // Update quality meter\n        document.querySelector(\u0027.quality-fill\u0027).style.width \u003d \u002790%\u0027;\n        document.querySelector(\u0027.hypothesis-quality span:last-child\u0027).textContent \u003d \u002790%\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/methodology-designer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Methodology Designer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .ai-suggestion {\n      background-color: rgba(255, 255, 255, 0.1);\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      color: white;\n    }\n    .ai-suggestion-header {\n      display: flex;\n      align-items: center;\n      margin-bottom: 10px;\n    }\n    .ai-icon {\n      width: 30px;\n      height: 30px;\n      background-color: #F09819;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      margin-right: 10px;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eMethodology Designer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 4 of 10: Design your experimental methodology\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 40%\" aria-valuenow\u003d\"40\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eDesign Your Methodology\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cform id\u003d\"methodologyForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"experimentType\" class\u003d\"form-label\"\u003eExperiment Type\u003c/label\u003e\n                \u003cselect class\u003d\"form-select\" id\u003d\"experimentType\"\u003e\n                  \u003coption selected\u003eSelect experiment type\u003c/option\u003e\n                  \u003coption value\u003d\"controlled\"\u003eControlled Experiment\u003c/option\u003e\n                  \u003coption value\u003d\"observational\"\u003eObservational Study\u003c/option\u003e\n                  \u003coption value\u003d\"survey\"\u003eSurvey Research\u003c/option\u003e\n                  \u003coption value\u003d\"case_study\"\u003eCase Study\u003c/option\u003e\n                  \u003coption value\u003d\"simulation\"\u003eSimulation\u003c/option\u003e\n                  \u003coption value\u003d\"field\"\u003eField Experiment\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"variables\" class\u003d\"form-label\"\u003eVariables\u003c/label\u003e\n                \u003cdiv class\u003d\"row mb-2\"\u003e\n                  \u003cdiv class\u003d\"col\"\u003e\n                    \u003cinput type\u003d\"text\" class\u003d\"form-control\" placeholder\u003d\"Independent Variable\" id\u003d\"independentVar\"\u003e\n                  \u003c/div\u003e\n                  \u003cdiv class\u003d\"col\"\u003e\n                    \u003cinput type\u003d\"text\" class\u003d\"form-control\" placeholder\u003d\"Dependent Variable\" id\u003d\"dependentVar\"\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n                \u003cdiv class\u003d\"row\"\u003e\n                  \u003cdiv class\u003d\"col\"\u003e\n                    \u003cinput type\u003d\"text\" class\u003d\"form-control\" placeholder\u003d\"Control Variable 1\" id\u003d\"controlVar1\"\u003e\n                  \u003c/div\u003e\n                  \u003cdiv class\u003d\"col\"\u003e\n                    \u003cinput type\u003d\"text\" class\u003d\"form-control\" placeholder\u003d\"Control Variable 2\" id\u003d\"controlVar2\"\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"sampleSize\" class\u003d\"form-label\"\u003eSample Size\u003c/label\u003e\n                \u003cinput type\u003d\"number\" class\u003d\"form-control\" id\u003d\"sampleSize\" placeholder\u003d\"Enter sample size\"\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"procedure\" class\u003d\"form-label\"\u003eProcedure\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"procedure\" rows\u003d\"5\" placeholder\u003d\"Describe your experimental procedure step by step\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"materials\" class\u003d\"form-label\"\u003eMaterials and Equipment\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"materials\" rows\u003d\"3\" placeholder\u003d\"List all materials and equipment needed\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"dataCollection\" class\u003d\"form-label\"\u003eData Collection Method\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"dataCollection\" rows\u003d\"3\" placeholder\u003d\"Describe how you will collect data\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"d-flex justify-content-between\"\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Data Gatherer\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAI Methodology Suggestions\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eExperimental Design\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eFor studying temperature effects on reaction rates, consider a controlled experiment with temperature as the independent variable and reaction rate as the dependent variable.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eSample Size Recommendation\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eTo achieve statistical significance, aim for at least 30 trials at each temperature level. Consider testing 5 different temperature levels.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eControl Variables\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eImportant control variables to maintain: concentration of reactants, pressure, catalyst presence, and reaction vessel size.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eData Collection Method\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eConsider using spectrophotometry to measure concentration changes over time, or gas volume measurements for reactions producing gases.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary w-100 mt-3\" id\u003d\"generateMethodology\"\u003eGenerate Complete Methodology\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027hypothesis-builder.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Save form data to localStorage\n        const experimentType \u003d document.getElementById(\u0027experimentType\u0027).value;\n        const independentVar \u003d document.getElementById(\u0027independentVar\u0027).value;\n        const dependentVar \u003d document.getElementById(\u0027dependentVar\u0027).value;\n        const controlVar1 \u003d document.getElementById(\u0027controlVar1\u0027).value;\n        const controlVar2 \u003d document.getElementById(\u0027controlVar2\u0027).value;\n        const sampleSize \u003d document.getElementById(\u0027sampleSize\u0027).value;\n        const procedure \u003d document.getElementById(\u0027procedure\u0027).value;\n        const materials \u003d document.getElementById(\u0027materials\u0027).value;\n        const dataCollection \u003d document.getElementById(\u0027dataCollection\u0027).value;\n        \n        localStorage.setItem(\u0027experimentType\u0027, experimentType);\n        localStorage.setItem(\u0027independentVar\u0027, independentVar);\n        localStorage.setItem(\u0027dependentVar\u0027, dependentVar);\n        localStorage.setItem(\u0027controlVar1\u0027, controlVar1);\n        localStorage.setItem(\u0027controlVar2\u0027, controlVar2);\n        localStorage.setItem(\u0027sampleSize\u0027, sampleSize);\n        localStorage.setItem(\u0027procedure\u0027, procedure);\n        localStorage.setItem(\u0027materials\u0027, materials);\n        localStorage.setItem(\u0027dataCollection\u0027, dataCollection);\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Data Gatherer\u0027);\n        \n        // Navigate to next step\n        window.location.href \u003d \u0027data-gatherer.html\u0027;\n      });\n      \n      // Handle generate methodology button\n      document.getElementById(\u0027generateMethodology\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call the LLM API\n        alert(\u0027AI would generate a complete methodology based on your experiment details.\u0027);\n        \n        // For demo purposes, fill in some sample data\n        document.getElementById(\u0027experimentType\u0027).value \u003d \u0027controlled\u0027;\n        document.getElementById(\u0027independentVar\u0027).value \u003d \u0027Temperature (°C)\u0027;\n        document.getElementById(\u0027dependentVar\u0027).value \u003d \u0027Reaction Rate (mol/L/s)\u0027;\n        document.getElementById(\u0027controlVar1\u0027).value \u003d \u0027Concentration of Reactants\u0027;\n        document.getElementById(\u0027controlVar2\u0027).value \u003d \u0027Pressure\u0027;\n        document.getElementById(\u0027sampleSize\u0027).value \u003d \u002730\u0027;\n        document.getElementById(\u0027procedure\u0027).value \u003d \u00271. Prepare 5 water baths at different temperatures (10°C, 20°C, 30°C, 40°C, 50°C).\\n2. Prepare identical reaction mixtures for each temperature.\\n3. Place reaction vessels in respective water baths and allow to equilibrate.\\n4. Add catalyst to start reaction and begin timing.\\n5. Measure product formation at 30-second intervals for 5 minutes.\\n6. Repeat each temperature condition 6 times for statistical validity.\u0027;\n        document.getElementById(\u0027materials\u0027).value \u003d \u0027- Temperature-controlled water baths\\n- Reaction vessels (50mL beakers)\\n- Thermometers\\n- Stopwatch\\n- Spectrophotometer\\n- Reactants A and B\\n- Catalyst\\n- Pipettes and graduated cylinders\u0027;\n        document.getElementById(\u0027dataCollection\u0027).value \u003d \u0027Measure reaction progress using spectrophotometry at 450nm wavelength. Record absorbance values at 30-second intervals and convert to concentration using a pre-established calibration curve.\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/data-gatherer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Data Gatherer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .data-source {\n      background-color: white;\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      transition: transform 0.3s ease;\n    }\n    .data-source:hover {\n      transform: translateY(-5px);\n      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n    }\n    .data-source h5 {\n      color: #4776E6;\n      margin-bottom: 10px;\n    }\n    .data-source p {\n      color: #666;\n      margin-bottom: 10px;\n    }\n    .data-source .badge {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      margin-right: 5px;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n    }\n    .table {\n      background-color: white;\n      border-radius: 10px;\n      overflow: hidden;\n    }\n    .table th {\n      background-color: #f8f9fa;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eData Gatherer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 5 of 10: Gather existing data for your experiment\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 50%\" aria-valuenow\u003d\"50\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eData Sources\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"dataSourceType\" class\u003d\"form-label\"\u003eSource Type\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"dataSourceType\"\u003e\n                \u003coption selected\u003eSelect data source type\u003c/option\u003e\n                \u003coption value\u003d\"published\"\u003ePublished Research Data\u003c/option\u003e\n                \u003coption value\u003d\"public\"\u003ePublic Datasets\u003c/option\u003e\n                \u003coption value\u003d\"simulated\"\u003eSimulated Data\u003c/option\u003e\n                \u003coption value\u003d\"manual\"\u003eManual Entry\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"dataFormat\" class\u003d\"form-label\"\u003eData Format\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"dataFormat\"\u003e\n                \u003coption selected\u003eSelect data format\u003c/option\u003e\n                \u003coption value\u003d\"csv\"\u003eCSV\u003c/option\u003e\n                \u003coption value\u003d\"excel\"\u003eExcel\u003c/option\u003e\n                \u003coption value\u003d\"json\"\u003eJSON\u003c/option\u003e\n                \u003coption value\u003d\"api\"\u003eAPI\u003c/option\u003e\n                \u003coption value\u003d\"manual\"\u003eManual Entry\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary w-100 mb-3\" id\u003d\"searchDataBtn\"\u003eSearch for Data\u003c/button\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Experiment Runner\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAvailable Datasets\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv id\u003d\"dataSources\"\u003e\n              \u003c!-- Sample data sources --\u003e\n              \u003cdiv class\u003d\"data-source\"\u003e\n                \u003ch5\u003eTemperature Effects on Reaction Rates Dataset\u003c/h5\u003e\n                \u003cp\u003eComprehensive dataset of reaction rates measured at various temperatures for common chemical reactions.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eChemistry\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eKinetics\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eTemperature\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"text-muted small\"\u003eSource: Journal of Chemical Kinetics, 2024 | 1,250 data points\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary import-data\"\u003eImport Dataset\u003c/button\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"data-source\"\u003e\n                \u003ch5\u003eArrhenius Parameters for Common Reactions\u003c/h5\u003e\n                \u003cp\u003eCollection of activation energies and pre-exponential factors for various chemical reactions.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eArrhenius Equation\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eActivation Energy\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"text-muted small\"\u003eSource: Chemical Reaction Database, 2023 | 500 reactions\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary import-data\"\u003eImport Dataset\u003c/button\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"data-source\"\u003e\n                \u003ch5\u003eTemperature-Controlled Reaction Experiments\u003c/h5\u003e\n                \u003cp\u003eRaw experimental data from temperature-controlled reaction chambers with precise measurements.\u003c/p\u003e\n                \u003cdiv class\u003d\"mb-2\"\u003e\n                  \u003cspan class\u003d\"badge\"\u003eExperimental\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eRaw Data\u003c/span\u003e\n                  \u003cspan class\u003d\"badge\"\u003eTime Series\u003c/span\u003e\n                \u003c/div\u003e\n                \u003cp class\u003d\"text-muted small\"\u003eSource: Open Science Framework, 2022 | 30 experiments\u003c/p\u003e\n                \u003cbutton class\u003d\"btn btn-sm btn-outline-primary import-data\"\u003eImport Dataset\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"card mt-4\" id\u003d\"importedDataCard\" style\u003d\"display: none;\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eImported Data Preview\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"table-responsive\"\u003e\n              \u003ctable class\u003d\"table table-striped\"\u003e\n                \u003cthead\u003e\n                  \u003ctr\u003e\n                    \u003cth\u003eTemperature (°C)\u003c/th\u003e\n                    \u003cth\u003eReaction Rate (mol/L/s)\u003c/th\u003e\n                    \u003cth\u003eActivation Energy (kJ/mol)\u003c/th\u003e\n                    \u003cth\u003ePre-exponential Factor\u003c/th\u003e\n                    \u003cth\u003eR² Value\u003c/th\u003e\n                  \u003c/tr\u003e\n                \u003c/thead\u003e\n                \u003ctbody id\u003d\"dataPreviewBody\"\u003e\n                  \u003c!-- Data will be loaded here --\u003e\n                \u003c/tbody\u003e\n              \u003c/table\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle search data button\n      document.getElementById(\u0027searchDataBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would search for datasets based on selected criteria\n        alert(\u0027Searching for datasets based on your criteria...\u0027);\n      });\n      \n      // Handle import dataset buttons\n      const importButtons \u003d document.querySelectorAll(\u0027.import-data\u0027);\n      importButtons.forEach(button \u003d\u003e {\n        button.addEventListener(\u0027click\u0027, function() {\n          // Show the imported data preview card\n          document.getElementById(\u0027importedDataCard\u0027).style.display \u003d \u0027block\u0027;\n          \n          // Generate sample data for preview\n          const dataPreviewBody \u003d document.getElementById(\u0027dataPreviewBody\u0027);\n          dataPreviewBody.innerHTML \u003d \u0027\u0027;\n          \n          // Sample data points\n          const sampleData \u003d [\n            { temp: 10, rate: 0.0015, energy: 45.2, factor: \u00271.2 × 10⁹\u0027, r2: 0.992 },\n            { temp: 20, rate: 0.0032, energy: 45.2, factor: \u00271.2 × 10⁹\u0027, r2: 0.995 },\n            { temp: 30, rate: 0.0068, energy: 45.2, factor: \u00271.2 × 10⁹\u0027, r2: 0.989 },\n            { temp: 40, rate: 0.0142, energy: 45.2, factor: \u00271.2 × 10⁹\u0027, r2: 0.991 },\n            { temp: 50, rate: 0.0295, energy: 45.2, factor: \u00271.2 × 10⁹\u0027, r2: 0.988 }\n          ];\n          \n          sampleData.forEach(data \u003d\u003e {\n            const row \u003d document.createElement(\u0027tr\u0027);\n            row.innerHTML \u003d `\n              \u003ctd\u003e${data.temp}\u003c/td\u003e\n              \u003ctd\u003e${data.rate}\u003c/td\u003e\n              \u003ctd\u003e${data.energy}\u003c/td\u003e\n              \u003ctd\u003e${data.factor}\u003c/td\u003e\n              \u003ctd\u003e${data.r2}\u003c/td\u003e\n            `;\n            dataPreviewBody.appendChild(row);\n          });\n          \n          // Disable the clicked button\n          this.disabled \u003d true;\n          this.textContent \u003d \u0027Imported\u0027;\n          this.classList.remove(\u0027btn-outline-primary\u0027);\n          this.classList.add(\u0027btn-success\u0027);\n        });\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027methodology-designer.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Experiment Runner\u0027);\n        window.location.href \u003d \u0027experiment-runner.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/experiment-runner.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Experiment Runner\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .code-editor {\n      background-color: #1e1e1e;\n      color: #d4d4d4;\n      font-family: \u0027Courier New\u0027, monospace;\n      padding: 15px;\n      border-radius: 8px;\n      margin-bottom: 15px;\n      height: 300px;\n      overflow-y: auto;\n    }\n    .code-line {\n      display: block;\n      line-height: 1.5;\n    }\n    .code-comment {\n      color: #6a9955;\n    }\n    .code-keyword {\n      color: #569cd6;\n    }\n    .code-string {\n      color: #ce9178;\n    }\n    .code-function {\n      color: #dcdcaa;\n    }\n    .code-number {\n      color: #b5cea8;\n    }\n    .console-output {\n      background-color: #2d2d2d;\n      color: #d4d4d4;\n      font-family: \u0027Courier New\u0027, monospace;\n      padding: 15px;\n      border-radius: 8px;\n      margin-bottom: 15px;\n      height: 200px;\n      overflow-y: auto;\n    }\n    .experiment-status {\n      display: flex;\n      align-items: center;\n      margin-bottom: 15px;\n    }\n    .status-indicator {\n      width: 15px;\n      height: 15px;\n      border-radius: 50%;\n      margin-right: 10px;\n    }\n    .status-ready {\n      background-color: #ffc107;\n    }\n    .status-running {\n      background-color: #007bff;\n      animation: pulse 1.5s infinite;\n    }\n    .status-completed {\n      background-color: #28a745;\n    }\n    .status-error {\n      background-color: #dc3545;\n    }\n    @keyframes pulse {\n      0% {\n        opacity: 1;\n      }\n      50% {\n        opacity: 0.5;\n      }\n      100% {\n        opacity: 1;\n      }\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eExperiment Runner\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 6 of 10: Run your experiment\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 60%\" aria-valuenow\u003d\"60\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eExperiment Code\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"experiment-status\"\u003e\n              \u003cdiv class\u003d\"status-indicator status-ready\" id\u003d\"statusIndicator\"\u003e\u003c/div\u003e\n              \u003cspan id\u003d\"statusText\"\u003eReady to run\u003c/span\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"code-editor\"\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Experiment: Effect of Temperature on Reaction Rates\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Generated by Project Hypatia AI\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-keyword\"\u003eimport\u003c/span\u003e numpy \u003cspan class\u003d\"code-keyword\"\u003eas\u003c/span\u003e np\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-keyword\"\u003eimport\u003c/span\u003e matplotlib.pyplot \u003cspan class\u003d\"code-keyword\"\u003eas\u003c/span\u003e plt\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-keyword\"\u003efrom\u003c/span\u003e scipy.optimize \u003cspan class\u003d\"code-keyword\"\u003eimport\u003c/span\u003e curve_fit\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Define the Arrhenius equation\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-keyword\"\u003edef\u003c/span\u003e \u003cspan class\u003d\"code-function\"\u003earrhenius\u003c/span\u003e(T, A, Ea, R\u003d\u003cspan class\u003d\"code-number\"\u003e8.314\u003c/span\u003e):\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e    \u003cspan class\u003d\"code-comment\"\u003e# T: temperature in Kelvin\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e    \u003cspan class\u003d\"code-comment\"\u003e# A: pre-exponential factor\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e    \u003cspan class\u003d\"code-comment\"\u003e# Ea: activation energy in J/mol\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e    \u003cspan class\u003d\"code-comment\"\u003e# R: gas constant in J/(mol·K)\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e    \u003cspan class\u003d\"code-keyword\"\u003ereturn\u003c/span\u003e A * np.exp(-Ea / (R * T))\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Generate temperature data points (convert from Celsius to Kelvin)\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003etemperatures_C \u003d np.array([\u003cspan class\u003d\"code-number\"\u003e10\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e20\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e30\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e40\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e50\u003c/span\u003e])\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003etemperatures_K \u003d temperatures_C + \u003cspan class\u003d\"code-number\"\u003e273.15\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Define true parameters for simulation\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003eA_true \u003d \u003cspan class\u003d\"code-number\"\u003e1.2e9\u003c/span\u003e  \u003cspan class\u003d\"code-comment\"\u003e# pre-exponential factor\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003eEa_true \u003d \u003cspan class\u003d\"code-number\"\u003e45200\u003c/span\u003e  \u003cspan class\u003d\"code-comment\"\u003e# activation energy in J/mol\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Calculate theoretical reaction rates\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003etrue_rates \u003d arrhenius(temperatures_K, A_true, Ea_true)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Add some random noise to simulate experimental error\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003enp.random.seed(\u003cspan class\u003d\"code-number\"\u003e42\u003c/span\u003e)  \u003cspan class\u003d\"code-comment\"\u003e# for reproducibility\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003enoise \u003d np.random.normal(\u003cspan class\u003d\"code-number\"\u003e0\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e0.0005\u003c/span\u003e, len(temperatures_K))\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003emeasured_rates \u003d true_rates + noise\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Fit the Arrhenius equation to the measured data\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003epopt, pcov \u003d curve_fit(\u003cspan class\u003d\"code-function\"\u003earrhenius\u003c/span\u003e, temperatures_K, measured_rates, p0\u003d[\u003cspan class\u003d\"code-number\"\u003e1e9\u003c/span\u003e, \u003cspan class\u003d\"code-number\"\u003e40000\u003c/span\u003e])\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003eA_fit, Ea_fit \u003d popt\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Calculate fitted rates\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003efitted_rates \u003d arrhenius(temperatures_K, A_fit, Ea_fit)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Calculate R-squared value\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003ess_tot \u003d np.sum((measured_rates - np.mean(measured_rates))**\u003cspan class\u003d\"code-number\"\u003e2\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003ess_res \u003d np.sum((measured_rates - fitted_rates)**\u003cspan class\u003d\"code-number\"\u003e2\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003er_squared \u003d \u003cspan class\u003d\"code-number\"\u003e1\u003c/span\u003e - (ss_res / ss_tot)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Save results to a file\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003eresults \u003d np.column_stack((temperatures_C, measured_rates, fitted_rates))\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003enp.savetxt(\u003cspan class\u003d\"code-string\"\u003e\u0027experiment_results.csv\u0027\u003c/span\u003e, results, delimiter\u003d\u003cspan class\u003d\"code-string\"\u003e\u0027,\u0027\u003c/span\u003e, header\u003d\u003cspan class\u003d\"code-string\"\u003e\u0027Temperature (C),Measured Rate,Fitted Rate\u0027\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-comment\"\u003e# Print results\u003c/span\u003e\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-function\"\u003eprint\u003c/span\u003e(\u003cspan class\u003d\"code-string\"\u003e\u0027Experiment Results:\u0027\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-function\"\u003eprint\u003c/span\u003e(\u003cspan class\u003d\"code-string\"\u003ef\u0027Fitted pre-exponential factor (A): {A_fit:.3e}\u0027\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-function\"\u003eprint\u003c/span\u003e(\u003cspan class\u003d\"code-string\"\u003ef\u0027Fitted activation energy (Ea): {Ea_fit:.1f} J/mol\u0027\u003c/span\u003e)\u003c/span\u003e\n              \u003cspan class\u003d\"code-line\"\u003e\u003cspan class\u003d\"code-function\"\u003eprint\u003c/span\u003e(\u003cspan class\u003d\"code-string\"\u003ef\u0027R-squared value: {r_squared:.4f}\u0027\u003c/span\u003e)\u003c/span\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mb-3\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"editCodeBtn\"\u003eEdit Code\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"runExperimentBtn\"\u003eRun Experiment\u003c/button\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"console-output\" id\u003d\"consoleOutput\" style\u003d\"display: none;\"\u003e\n              \u003c!-- Console output will appear here --\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\" disabled\u003eNext: Data Analyzer\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eExperiment Parameters\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"experimentType\" class\u003d\"form-label\"\u003eExperiment Type\u003c/label\u003e\n              \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"experimentType\" value\u003d\"Controlled Experiment\" readonly\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"independentVar\" class\u003d\"form-label\"\u003eIndependent Variable\u003c/label\u003e\n              \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"independentVar\" value\u003d\"Temperature (°C)\" readonly\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"dependentVar\" class\u003d\"form-label\"\u003eDependent Variable\u003c/label\u003e\n              \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"dependentVar\" value\u003d\"Reaction Rate (mol/L/s)\" readonly\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"controlVars\" class\u003d\"form-label\"\u003eControl Variables\u003c/label\u003e\n              \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"controlVars\" value\u003d\"Concentration, Pressure\" readonly\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"sampleSize\" class\u003d\"form-label\"\u003eSample Size\u003c/label\u003e\n              \u003cinput type\u003d\"text\" class\u003d\"form-control\" id\u003d\"sampleSize\" value\u003d\"30\" readonly\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"experimentNotes\" class\u003d\"form-label\"\u003eNotes\u003c/label\u003e\n              \u003ctextarea class\u003d\"form-control\" id\u003d\"experimentNotes\" rows\u003d\"3\" placeholder\u003d\"Add notes about your experiment\"\u003e\u003c/textarea\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle edit code button\n      document.getElementById(\u0027editCodeBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        alert(\u0027In a production environment, this would open a code editor for modifying the experiment code.\u0027);\n      });\n      \n      // Handle run experiment button\n      document.getElementById(\u0027runExperimentBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Show console output\n        const consoleOutput \u003d document.getElementById(\u0027consoleOutput\u0027);\n        consoleOutput.style.display \u003d \u0027block\u0027;\n        consoleOutput.innerHTML \u003d \u0027\u0027;\n        \n        // Update status\n        const statusIndicator \u003d document.getElementById(\u0027statusIndicator\u0027);\n        const statusText \u003d document.getElementById(\u0027statusText\u0027);\n        statusIndicator.className \u003d \u0027status-indicator status-running\u0027;\n        statusText.textContent \u003d \u0027Running experiment...\u0027;\n        \n        // Disable run button\n        this.disabled \u003d true;\n        \n        // Simulate experiment running\n        setTimeout(function() {\n          // Add output lines with delay to simulate real-time output\n          addOutputLine(\u0027Initializing experiment...\u0027);\n        }, 500);\n        \n        setTimeout(function() {\n          addOutputLine(\u0027Loading temperature data points...\u0027);\n          addOutputLine(\u0027Temperatures (°C): [10, 20, 30, 40, 50]\u0027);\n          addOutputLine(\u0027Temperatures (K): [283.15, 293.15, 303.15, 313.15, 323.15]\u0027);\n        }, 1500);\n        \n        setTimeout(function() {\n          addOutputLine(\u0027Calculating theoretical reaction rates...\u0027);\n          addOutputLine(\u0027Simulating experimental measurements with random noise...\u0027);\n        }, 2500);\n        \n        setTimeout(function() {\n          addOutputLine(\u0027Fitting Arrhenius equation to measured data...\u0027);\n          addOutputLine(\u0027Calculating R-squared value...\u0027);\n        }, 3500);\n        \n        setTimeout(function() {\n          addOutputLine(\u0027Saving results to experiment_results.csv...\u0027);\n        }, 4500);\n        \n        setTimeout(function() {\n          addOutputLine(\u0027Experiment Results:\u0027);\n          addOutputLine(\u0027Fitted pre-exponential factor (A): 1.198e+09\u0027);\n          addOutputLine(\u0027Fitted activation energy (Ea): 45187.3 J/mol\u0027);\n          addOutputLine(\u0027R-squared value: 0.9982\u0027);\n          addOutputLine(\u0027Experiment completed successfully!\u0027);\n          \n          // Update status\n          statusIndicator.className \u003d \u0027status-indicator status-completed\u0027;\n          statusText.textContent \u003d \u0027Experiment completed\u0027;\n          \n          // Enable next step button\n          document.getElementById(\u0027nextStepBtn\u0027).disabled \u003d false;\n          \n          // Re-enable run button\n          document.getElementById(\u0027runExperimentBtn\u0027).disabled \u003d false;\n        }, 5500);\n      });\n      \n      function addOutputLine(text) {\n        const consoleOutput \u003d document.getElementById(\u0027consoleOutput\u0027);\n        const line \u003d document.createElement(\u0027div\u0027);\n        line.textContent \u003d text;\n        consoleOutput.appendChild(line);\n        consoleOutput.scrollTop \u003d consoleOutput.scrollHeight;\n      }\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027data-gatherer.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Data Analyzer\u0027);\n        window.location.href \u003d \u0027data-analyzer.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/data-analyzer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Data Analyzer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cscript src\u003d\"https://cdn.plot.ly/plotly-latest.min.js\"\u003e\u003c/script\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .chart-container {\n      background-color: white;\n      border-radius: 10px;\n      padding: 20px;\n      margin-bottom: 20px;\n      height: 400px;\n    }\n    .form-select {\n      border-radius: 8px;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eData Analyzer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 7 of 10: Analyze your experiment data\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 70%\" aria-valuenow\u003d\"70\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAnalysis Options\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"chartType\" class\u003d\"form-label\"\u003eChart Type\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"chartType\"\u003e\n                \u003coption value\u003d\"scatter\" selected\u003eScatter Plot\u003c/option\u003e\n                \u003coption value\u003d\"line\"\u003eLine Chart\u003c/option\u003e\n                \u003coption value\u003d\"bar\"\u003eBar Chart\u003c/option\u003e\n                \u003coption value\u003d\"histogram\"\u003eHistogram\u003c/option\u003e\n                \u003coption value\u003d\"boxplot\"\u003eBox Plot\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"xAxis\" class\u003d\"form-label\"\u003eX-Axis\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"xAxis\"\u003e\n                \u003coption value\u003d\"temperature\" selected\u003eTemperature (°C)\u003c/option\u003e\n                \u003coption value\u003d\"rate\"\u003eReaction Rate (mol/L/s)\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"yAxis\" class\u003d\"form-label\"\u003eY-Axis\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"yAxis\"\u003e\n                \u003coption value\u003d\"rate\" selected\u003eReaction Rate (mol/L/s)\u003c/option\u003e\n                \u003coption value\u003d\"temperature\"\u003eTemperature (°C)\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary w-100 mb-3\" id\u003d\"generateChartBtn\"\u003eGenerate Chart\u003c/button\u003e\n            \u003cdiv class\u003d\"mb-3\"\u003e\n              \u003clabel for\u003d\"statisticalTest\" class\u003d\"form-label\"\u003eStatistical Test\u003c/label\u003e\n              \u003cselect class\u003d\"form-select\" id\u003d\"statisticalTest\"\u003e\n                \u003coption selected\u003eSelect test\u003c/option\u003e\n                \u003coption value\u003d\"t_test\"\u003eT-Test\u003c/option\u003e\n                \u003coption value\u003d\"anova\"\u003eANOVA\u003c/option\u003e\n                \u003coption value\u003d\"chi_squared\"\u003eChi-Squared\u003c/option\u003e\n                \u003coption value\u003d\"correlation\"\u003eCorrelation\u003c/option\u003e\n                \u003coption value\u003d\"regression\"\u003eRegression Analysis\u003c/option\u003e\n              \u003c/select\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary w-100\" id\u003d\"runTestBtn\"\u003eRun Statistical Test\u003c/button\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Conclusion Drawer\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eData Visualization\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv id\u003d\"chartContainer\" class\u003d\"chart-container\"\u003e\u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class\u003d\"card mt-4\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eStatistical Analysis Results\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv id\u003d\"analysisResults\"\u003e\n              \u003cp class\u003d\"text-muted\"\u003eRun a statistical test to see results here.\u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n\n      // Sample data (in a real app, this would come from the previous step)\n      const experimentData \u003d {\n        temperatures: [10, 20, 30, 40, 50],\n        measured_rates: [0.0015, 0.0032, 0.0068, 0.0142, 0.0295],\n        fitted_rates: [0.0016, 0.0033, 0.0067, 0.0135, 0.0273]\n      };\n\n      // Handle generate chart button\n      document.getElementById(\u0027generateChartBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        const chartType \u003d document.getElementById(\u0027chartType\u0027).value;\n        const xAxisData \u003d document.getElementById(\u0027xAxis\u0027).value \u003d\u003d\u003d \u0027temperature\u0027 ? experimentData.temperatures : experimentData.measured_rates;\n        const yAxisData \u003d document.getElementById(\u0027yAxis\u0027).value \u003d\u003d\u003d \u0027rate\u0027 ? experimentData.measured_rates : experimentData.temperatures;\n        const xAxisTitle \u003d document.getElementById(\u0027xAxis\u0027).selectedOptions[0].text;\n        const yAxisTitle \u003d document.getElementById(\u0027yAxis\u0027).selectedOptions[0].text;\n\n        let trace1 \u003d {\n          x: xAxisData,\n          y: yAxisData,\n          mode: \u0027markers\u0027,\n          type: chartType,\n          name: \u0027Measured Data\u0027\n        };\n\n        let trace2 \u003d {\n            x: xAxisData,\n            y: experimentData.fitted_rates, // Assuming x is temperature for fitted rates\n            mode: \u0027lines\u0027,\n            type: \u0027scatter\u0027, // Fitted data usually shown as a line\n            name: \u0027Fitted Model\u0027\n        };\n\n        let data \u003d [trace1];\n        if (chartType \u003d\u003d\u003d \u0027scatter\u0027 || chartType \u003d\u003d\u003d \u0027line\u0027) {\n            data.push(trace2);\n        }\n\n        const layout \u003d {\n          title: `${yAxisTitle} vs. ${xAxisTitle}`,\n          xaxis: { title: xAxisTitle },\n          yaxis: { title: yAxisTitle },\n          margin: { t: 50, b: 50, l: 60, r: 30 },\n          paper_bgcolor: \u0027rgba(0,0,0,0)\u0027,\n          plot_bgcolor: \u0027rgba(0,0,0,0)\u0027,\n          font: { color: \u0027#333\u0027 }\n        };\n\n        Plotly.newPlot(\u0027chartContainer\u0027, data, layout);\n      });\n\n      // Handle run statistical test button\n      document.getElementById(\u0027runTestBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        const testType \u003d document.getElementById(\u0027statisticalTest\u0027).value;\n        const resultsContainer \u003d document.getElementById(\u0027analysisResults\u0027);\n        resultsContainer.innerHTML \u003d \u0027\u0027; // Clear previous results\n\n        let resultText \u003d \u0027\u0027;\n        if (testType \u003d\u003d\u003d \u0027regression\u0027) {\n          // Simplified regression example (using predefined R-squared)\n          const r_squared \u003d 0.9982; // From previous step simulation\n          resultText \u003d `\n            \u003ch5\u003eRegression Analysis Results\u003c/h5\u003e\n            \u003cp\u003e\u003cstrong\u003eR-squared:\u003c/strong\u003e ${r_squared.toFixed(4)}\u003c/p\u003e\n            \u003cp\u003e\u003cstrong\u003eInterpretation:\u003c/strong\u003e The model explains ${ (r_squared * 100).toFixed(2)}% of the variance in the reaction rate.\u003c/p\u003e\n            \u003cp\u003e\u003cstrong\u003eFitted Parameters (from experiment):\u003c/strong\u003e\u003c/p\u003e\n            \u003cul\u003e\n              \u003cli\u003ePre-exponential factor (A): 1.198e+09\u003c/li\u003e\n              \u003cli\u003eActivation energy (Ea): 45187.3 J/mol\u003c/li\u003e\n            \u003c/ul\u003e\n          `;\n        } else {\n          resultText \u003d `\u003cp\u003eResults for \u003cstrong\u003e${testType}\u003c/strong\u003e would be displayed here. This is a placeholder.\u003c/p\u003e`;\n        }\n        resultsContainer.innerHTML \u003d resultText;\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027experiment-runner.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Conclusion Drawer\u0027);\n        window.location.href \u003d \u0027conclusion-drawer.html\u0027;\n      });\n\n      // Generate initial chart on load\n      document.getElementById(\u0027generateChartBtn\u0027).click();\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/conclusion-drawer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Conclusion Drawer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .ai-suggestion {\n      background-color: rgba(255, 255, 255, 0.1);\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      color: white;\n    }\n    .ai-suggestion-header {\n      display: flex;\n      align-items: center;\n      margin-bottom: 10px;\n    }\n    .ai-icon {\n      width: 30px;\n      height: 30px;\n      background-color: #F09819;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      margin-right: 10px;\n    }\n    .conclusion-status {\n      display: inline-block;\n      padding: 8px 15px;\n      border-radius: 50px;\n      font-weight: bold;\n      margin-bottom: 15px;\n    }\n    .status-supports {\n      background-color: #28a745;\n      color: white;\n    }\n    .status-refutes {\n      background-color: #dc3545;\n      color: white;\n    }\n    .status-inconclusive {\n      background-color: #ffc107;\n      color: black;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003eConclusion Drawer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 8 of 10: Draw conclusions from your experiment\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 80%\" aria-valuenow\u003d\"80\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eDraw Your Conclusion\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"mb-4\"\u003e\n              \u003ch6\u003eHypothesis Statement:\u003c/h6\u003e\n              \u003cp class\u003d\"border p-3 rounded bg-light\"\u003eIf the temperature of the reaction environment increases from 10°C to 50°C, then the rate of the chemical reaction will increase according to the Arrhenius equation, with approximately a doubling of rate for each 10°C increase.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"mb-4\"\u003e\n              \u003ch6\u003eConclusion Status:\u003c/h6\u003e\n              \u003cdiv class\u003d\"conclusion-status status-supports\"\u003eSupports Hypothesis\u003c/div\u003e\n            \u003c/div\u003e\n            \u003cform id\u003d\"conclusionForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"conclusionSummary\" class\u003d\"form-label\"\u003eConclusion Summary\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"conclusionSummary\" rows\u003d\"4\" placeholder\u003d\"Summarize your conclusion based on the experimental results\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"evidenceSupport\" class\u003d\"form-label\"\u003eEvidence Supporting Conclusion\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"evidenceSupport\" rows\u003d\"3\" placeholder\u003d\"List the evidence that supports your conclusion\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"limitations\" class\u003d\"form-label\"\u003eLimitations and Uncertainties\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"limitations\" rows\u003d\"3\" placeholder\u003d\"Describe any limitations or uncertainties in your experiment\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"futureDirections\" class\u003d\"form-label\"\u003eFuture Research Directions\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"futureDirections\" rows\u003d\"3\" placeholder\u003d\"Suggest directions for future research based on your findings\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Peer Reviewer\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAI Suggestions\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eConclusion Structure\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eA strong conclusion should restate your hypothesis, summarize key findings, explain how the data supports or refutes your hypothesis, and discuss implications.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eSuggested Conclusion\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eThe experimental results strongly support the hypothesis. The reaction rates increased with temperature following the Arrhenius equation, with an R² value of 0.9982 indicating an excellent fit to the model.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eLimitations to Consider\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cul\u003e\n                \u003cli\u003eLimited temperature range (10-50°C)\u003c/li\u003e\n                \u003cli\u003eSingle reaction type studied\u003c/li\u003e\n                \u003cli\u003eControlled laboratory conditions\u003c/li\u003e\n              \u003c/ul\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary w-100 mt-3\" id\u003d\"generateConclusionBtn\"\u003eGenerate Complete Conclusion\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle generate conclusion button\n      document.getElementById(\u0027generateConclusionBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call the LLM API\n        alert(\u0027AI would generate a complete conclusion based on your experiment results.\u0027);\n        \n        // For demo purposes, fill in some sample data\n        document.getElementById(\u0027conclusionSummary\u0027).value \u003d \u0027The experimental results strongly support the hypothesis that reaction rates increase with temperature according to the Arrhenius equation. The data showed a clear exponential relationship between temperature and reaction rate, with rates approximately doubling for each 10°C increase in temperature. The fitted Arrhenius parameters (A \u003d 1.198e+09, Ea \u003d 45187.3 J/mol) closely match theoretical expectations, and the high R² value of 0.9982 indicates that the model explains nearly all of the variance in the observed data.\u0027;\n        \n        document.getElementById(\u0027evidenceSupport\u0027).value \u003d \u00271. The reaction rates increased exponentially with temperature as predicted.\\n2. The R² value of 0.9982 indicates an excellent fit to the Arrhenius model.\\n3. The calculated activation energy (45.2 kJ/mol) is consistent with typical values for this type of reaction.\\n4. The data points show a clear trend with minimal scatter.\u0027;\n        \n        document.getElementById(\u0027limitations\u0027).value \u003d \u00271. The experiment was conducted over a limited temperature range (10-50°C).\\n2. Only one type of reaction was studied.\\n3. The experiment was performed under controlled laboratory conditions that may not reflect real-world scenarios.\\n4. The sample size was relatively small (5 temperature points).\u0027;\n        \n        document.getElementById(\u0027futureDirections\u0027).value \u003d \u00271. Expand the temperature range to test the limits of the Arrhenius relationship.\\n2. Study different types of reactions to determine if the relationship holds across various chemical systems.\\n3. Investigate the effect of catalysts on the temperature-rate relationship.\\n4. Develop mathematical models that account for additional variables beyond temperature.\u0027;\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027data-analyzer.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Peer Reviewer\u0027);\n        window.location.href \u003d \u0027peer-reviewer.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/peer-reviewer.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Peer Reviewer\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .ai-suggestion {\n      background-color: rgba(255, 255, 255, 0.1);\n      border-radius: 10px;\n      padding: 15px;\n      margin-bottom: 15px;\n      color: white;\n    }\n    .ai-suggestion-header {\n      display: flex;\n      align-items: center;\n      margin-bottom: 10px;\n    }\n    .ai-icon {\n      width: 30px;\n      height: 30px;\n      background-color: #F09819;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      margin-right: 10px;\n    }\n    .review-comment {\n      background-color: #f8f9fa;\n      border-left: 4px solid #4776E6;\n      padding: 15px;\n      margin-bottom: 15px;\n      border-radius: 0 8px 8px 0;\n    }\n    .review-comment h6 {\n      color: #4776E6;\n      margin-bottom: 10px;\n    }\n    .review-score {\n      display: flex;\n      align-items: center;\n      margin-bottom: 20px;\n    }\n    .score-label {\n      width: 150px;\n      font-weight: bold;\n    }\n    .score-value {\n      width: 50px;\n      text-align: center;\n      font-weight: bold;\n    }\n    .score-bar {\n      flex-grow: 1;\n      height: 10px;\n      background-color: #e9ecef;\n      border-radius: 5px;\n      margin: 0 10px;\n      overflow: hidden;\n    }\n    .score-fill {\n      height: 100%;\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003ePeer Reviewer\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 9 of 10: Get peer review feedback on your research\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 90%\" aria-valuenow\u003d\"90\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003ePeer Review Feedback\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"mb-4\"\u003e\n              \u003ch6\u003eOverall Assessment:\u003c/h6\u003e\n              \u003cdiv class\u003d\"review-score\"\u003e\n                \u003cspan class\u003d\"score-label\"\u003eScientific Merit:\u003c/span\u003e\n                \u003cdiv class\u003d\"score-bar\"\u003e\n                  \u003cdiv class\u003d\"score-fill\" style\u003d\"width: 85%\"\u003e\u003c/div\u003e\n                \u003c/div\u003e\n                \u003cspan class\u003d\"score-value\"\u003e8.5/10\u003c/span\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"review-score\"\u003e\n                \u003cspan class\u003d\"score-label\"\u003eMethodology:\u003c/span\u003e\n                \u003cdiv class\u003d\"score-bar\"\u003e\n                  \u003cdiv class\u003d\"score-fill\" style\u003d\"width: 90%\"\u003e\u003c/div\u003e\n                \u003c/div\u003e\n                \u003cspan class\u003d\"score-value\"\u003e9.0/10\u003c/span\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"review-score\"\u003e\n                \u003cspan class\u003d\"score-label\"\u003eData Analysis:\u003c/span\u003e\n                \u003cdiv class\u003d\"score-bar\"\u003e\n                  \u003cdiv class\u003d\"score-fill\" style\u003d\"width: 80%\"\u003e\u003c/div\u003e\n                \u003c/div\u003e\n                \u003cspan class\u003d\"score-value\"\u003e8.0/10\u003c/span\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"review-score\"\u003e\n                \u003cspan class\u003d\"score-label\"\u003eConclusion Validity:\u003c/span\u003e\n                \u003cdiv class\u003d\"score-bar\"\u003e\n                  \u003cdiv class\u003d\"score-fill\" style\u003d\"width: 85%\"\u003e\u003c/div\u003e\n                \u003c/div\u003e\n                \u003cspan class\u003d\"score-value\"\u003e8.5/10\u003c/span\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \n            \u003cdiv class\u003d\"review-comment\"\u003e\n              \u003ch6\u003eStrengths\u003c/h6\u003e\n              \u003cp\u003eThe experiment is well-designed with a clear hypothesis that is testable and specific. The methodology is appropriate for investigating the relationship between temperature and reaction rate. The data analysis is thorough, with good use of the Arrhenius equation to model the relationship. The conclusion is well-supported by the experimental results.\u003c/p\u003e\n            \u003c/div\u003e\n            \n            \u003cdiv class\u003d\"review-comment\"\u003e\n              \u003ch6\u003eAreas for Improvement\u003c/h6\u003e\n              \u003cp\u003eThe temperature range could be expanded to test the limits of the Arrhenius relationship. Additional control variables could be considered to strengthen the experimental design. The statistical analysis could benefit from more rigorous uncertainty quantification. The discussion of limitations could be more comprehensive.\u003c/p\u003e\n            \u003c/div\u003e\n            \n            \u003cdiv class\u003d\"review-comment\"\u003e\n              \u003ch6\u003eSpecific Recommendations\u003c/h6\u003e\n              \u003col\u003e\n                \u003cli\u003eConsider including error bars in your data visualization to represent experimental uncertainty.\u003c/li\u003e\n                \u003cli\u003eExpand the discussion of how your results compare to previous studies in the literature.\u003c/li\u003e\n                \u003cli\u003eProvide more detail on the potential applications of your findings.\u003c/li\u003e\n                \u003cli\u003eConsider additional statistical tests to further validate your conclusions.\u003c/li\u003e\n              \u003c/ol\u003e\n            \u003c/div\u003e\n            \n            \u003cform id\u003d\"responseForm\"\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"reviewResponse\" class\u003d\"form-label\"\u003eYour Response to Reviewer\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"reviewResponse\" rows\u003d\"4\" placeholder\u003d\"Respond to the reviewer\u0027s comments and suggestions\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"mb-3\"\u003e\n                \u003clabel for\u003d\"plannedRevisions\" class\u003d\"form-label\"\u003ePlanned Revisions\u003c/label\u003e\n                \u003ctextarea class\u003d\"form-control\" id\u003d\"plannedRevisions\" rows\u003d\"3\" placeholder\u003d\"Describe the revisions you plan to make based on the feedback\"\u003e\u003c/textarea\u003e\n              \u003c/div\u003e\n              \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n                \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"nextStepBtn\"\u003eNext: Publication Exporter\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/form\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eAI Suggestions\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eResponse Strategy\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eWhen responding to peer review, thank the reviewer for their feedback, address each point specifically, and explain how you plan to incorporate their suggestions.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eSuggested Response\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cp\u003eThank you for your thoughtful review. We appreciate your recognition of our experimental design and analysis. We agree that expanding the temperature range would strengthen our findings and will address this in our revisions.\u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"ai-suggestion\"\u003e\n              \u003cdiv class\u003d\"ai-suggestion-header\"\u003e\n                \u003cdiv class\u003d\"ai-icon\"\u003eAI\u003c/div\u003e\n                \u003ch6 class\u003d\"mb-0\"\u003eRevision Priorities\u003c/h6\u003e\n              \u003c/div\u003e\n              \u003cul\u003e\n                \u003cli\u003eAdd error bars to data visualizations\u003c/li\u003e\n                \u003cli\u003eExpand literature comparison section\u003c/li\u003e\n                \u003cli\u003eEnhance discussion of applications\u003c/li\u003e\n                \u003cli\u003eInclude additional statistical validation\u003c/li\u003e\n              \u003c/ul\u003e\n            \u003c/div\u003e\n            \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary w-100 mt-3\" id\u003d\"generateResponseBtn\"\u003eGenerate Complete Response\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cscript src\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js\"\u003e\u003c/script\u003e\n  \u003cscript\u003e\n    // Check if user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle generate response button\n      document.getElementById(\u0027generateResponseBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // In a real app, this would call the LLM API\n        alert(\u0027AI would generate a complete response to the reviewer based on your experiment.\u0027);\n        \n        // For demo purposes, fill in some sample data\n        document.getElementById(\u0027reviewResponse\u0027).value \u003d \u0027Thank you for your thoughtful and constructive review of our research. We appreciate your recognition of the strengths in our experimental design, methodology, and data analysis. Your feedback provides valuable insights that will help us improve the quality and rigor of our work.\\n\\nWe agree that expanding the temperature range would provide a more comprehensive test of the Arrhenius relationship, particularly at temperature extremes where deviations might occur. We also acknowledge the need for more rigorous uncertainty quantification and a more comprehensive discussion of limitations.\u0027;\n        \n        document.getElementById(\u0027plannedRevisions\u0027).value \u003d \u00271. We will add error bars to all data visualizations to represent experimental uncertainty.\\n2. We will expand our discussion to include comparisons with three recent studies on similar reaction systems.\\n3. We will add a new section discussing potential applications in industrial catalysis and biochemical processes.\\n4. We will perform additional statistical analysis, including residual analysis and confidence intervals for the Arrhenius parameters.\u0027;\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027conclusion-drawer.html\u0027;\n      });\n      \n      // Handle next step button\n      document.getElementById(\u0027nextStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        localStorage.setItem(\u0027currentStep\u0027, \u0027Published\u0027);\n        window.location.href \u003d \u0027publication-exporter.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e\n--- START OF FILE public/workflow/publication-exporter.html ---\n\n\u003c!DOCTYPE html\u003e\n\u003chtml lang\u003d\"en\"\u003e\n\u003chead\u003e\n  \u003cmeta charset\u003d\"UTF-8\"\u003e\n  \u003cmeta name\u003d\"viewport\" content\u003d\"width\u003ddevice-width, initial-scale\u003d1.0\"\u003e\n  \u003ctitle\u003eProject Hypatia - Publication Exporter\u003c/title\u003e\n  \u003clink href\u003d\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel\u003d\"stylesheet\"\u003e\n  \u003cstyle\u003e\n    body {\n      background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);\n      min-height: 100vh;\n      font-family: \u0027Segoe UI\u0027, Tahoma, Geneva, Verdana, sans-serif;\n    }\n    .container {\n      max-width: 1200px;\n      margin: 0 auto;\n      padding: 20px;\n    }\n    .navbar {\n      background-color: rgba(255, 255, 255, 0.1);\n      backdrop-filter: blur(10px);\n      border-radius: 10px;\n      margin-bottom: 30px;\n    }\n    .navbar-brand {\n      font-weight: bold;\n      color: white !important;\n      font-size: 24px;\n    }\n    .nav-link {\n      color: rgba(255, 255, 255, 0.8) !important;\n      font-size: 16px;\n      margin: 0 10px;\n    }\n    .nav-link:hover {\n      color: white !important;\n    }\n    .workflow-header {\n      color: white;\n      margin-bottom: 30px;\n    }\n    .card {\n      border-radius: 15px;\n      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);\n      border: none;\n      margin-bottom: 20px;\n      overflow: hidden;\n    }\n    .card-header {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      color: white;\n      border-bottom: none;\n      padding: 15px 20px;\n    }\n    .card-body {\n      padding: 20px;\n    }\n    .btn-primary {\n      background: linear-gradient(to right, #4776E6, #8E54E9);\n      border: none;\n      border-radius: 8px;\n      padding: 10px 20px;\n      font-weight: bold;\n    }\n    .btn-primary:hover {\n      background: linear-gradient(to right, #3a67d4, #7d48d6);\n    }\n    .progress-container {\n      margin-bottom: 30px;\n    }\n    .progress {\n      height: 10px;\n      border-radius: 5px;\n      background-color: rgba(255, 255, 255, 0.2);\n    }\n    .progress-bar {\n      background: linear-gradient(to right, #FF512F, #F09819);\n    }\n    .step-indicator {\n      display: flex;\n      justify-content: space-between;\n      margin-top: 10px;\n    }\n    .step {\n      color: white;\n      font-size: 12px;\n      text-align: center;\n      position: relative;\n      width: 10%;\n    }\n    .step.active {\n      font-weight: bold;\n      color: #F09819;\n    }\n    .form-control {\n      border-radius: 8px;\n      padding: 12px;\n      margin-bottom: 15px;\n    }\n    .form-label {\n      font-weight: bold;\n    }\n    .paper-preview {\n      background-color: white;\n      border-radius: 10px;\n      padding: 30px;\n      margin-bottom: 20px;\n      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n    }\n    .paper-title {\n      font-size: 24px;\n      font-weight: bold;\n      margin-bottom: 15px;\n      text-align: center;\n    }\n    .paper-authors {\n      font-size: 16px;\n      margin-bottom: 20px;\n      text-align: center;\n      color: #666;\n    }\n    .paper-abstract {\n      font-style: italic;\n      margin-bottom: 20px;\n      padding: 15px;\n      background-color: #f8f9fa;\n      border-radius: 8px;\n    }\n    .paper-section {\n      margin-bottom: 20px;\n    }\n    .paper-section h3 {\n      font-size: 18px;\n      font-weight: bold;\n      margin-bottom: 10px;\n      color: #4776E6;\n    }\n    .export-option {\n      display: flex;\n      align-items: center;\n      padding: 15px;\n      border: 1px solid #dee2e6;\n      border-radius: 8px;\n      margin-bottom: 15px;\n      cursor: pointer;\n      transition: all 0.3s ease;\n    }\n    .export-option:hover {\n      border-color: #4776E6;\n      background-color: #f8f9fa;\n    }\n    .export-option.selected {\n      border-color: #4776E6;\n      background-color: rgba(71, 118, 230, 0.1);\n    }\n    .export-icon {\n      width: 40px;\n      height: 40px;\n      background-color: #4776E6;\n      border-radius: 8px;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      color: white;\n      font-weight: bold;\n      margin-right: 15px;\n    }\n    .export-details {\n      flex-grow: 1;\n    }\n    .export-details h5 {\n      margin-bottom: 5px;\n    }\n    .export-details p {\n      margin-bottom: 0;\n      color: #666;\n      font-size: 14px;\n    }\n  \u003c/style\u003e\n\u003c/head\u003e\n\u003cbody\u003e\n  \u003cdiv class\u003d\"container\"\u003e\n    \u003cnav class\u003d\"navbar navbar-expand-lg\"\u003e\n      \u003cdiv class\u003d\"container-fluid\"\u003e\n        \u003ca class\u003d\"navbar-brand\" href\u003d\"../index.html\"\u003eProject Hypatia\u003c/a\u003e\n        \u003cbutton class\u003d\"navbar-toggler\" type\u003d\"button\" data-bs-toggle\u003d\"collapse\" data-bs-target\u003d\"#navbarNav\" aria-controls\u003d\"navbarNav\" aria-expanded\u003d\"false\" aria-label\u003d\"Toggle navigation\"\u003e\n          \u003cspan class\u003d\"navbar-toggler-icon\"\u003e\u003c/span\u003e\n        \u003c/button\u003e\n        \u003cdiv class\u003d\"collapse navbar-collapse\" id\u003d\"navbarNav\"\u003e\n          \u003cul class\u003d\"navbar-nav ms-auto\"\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"../dashboard.html\"\u003eDashboard\u003c/a\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003cspan class\u003d\"nav-link\" id\u003d\"userEmail\"\u003e\u003c/span\u003e\n            \u003c/li\u003e\n            \u003cli class\u003d\"nav-item\"\u003e\n              \u003ca class\u003d\"nav-link\" href\u003d\"#\" id\u003d\"logoutBtn\"\u003eLogout\u003c/a\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/nav\u003e\n\n    \u003cdiv class\u003d\"workflow-header\"\u003e\n      \u003ch1\u003ePublication Exporter\u003c/h1\u003e\n      \u003cp class\u003d\"lead\"\u003eStep 10 of 10: Export your research for publication\u003c/p\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"progress-container\"\u003e\n      \u003cdiv class\u003d\"progress\"\u003e\n        \u003cdiv class\u003d\"progress-bar\" role\u003d\"progressbar\" style\u003d\"width: 100%\" aria-valuenow\u003d\"100\" aria-valuemin\u003d\"0\" aria-valuemax\u003d\"100\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"step-indicator\"\u003e\n        \u003cdiv class\u003d\"step\"\u003eQuestion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eResearch\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eHypothesis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eMethodology\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eData\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eExperiment\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eAnalysis\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eConclusion\u003c/div\u003e\n        \u003cdiv class\u003d\"step\"\u003eReview\u003c/div\u003e\n        \u003cdiv class\u003d\"step active\"\u003ePublication\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class\u003d\"row\"\u003e\n      \u003cdiv class\u003d\"col-lg-8\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003ePaper Preview\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"paper-preview\"\u003e\n              \u003cdiv class\u003d\"paper-title\"\u003eEffect of Temperature on Reaction Rates: An Experimental Study\u003c/div\u003e\n              \u003cdiv class\u003d\"paper-authors\"\u003eAuthor: \u003cspan id\u003d\"paperAuthor\"\u003e\u003c/span\u003e\u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-abstract\"\u003e\n                \u003cstrong\u003eAbstract:\u003c/strong\u003e This study investigates the relationship between temperature and reaction rate in a controlled chemical system. Using the Arrhenius equation as a theoretical framework, we conducted experiments across a temperature range of 10-50°C. Our results demonstrate a clear exponential relationship between temperature and reaction rate, with an activation energy of 45.2 kJ/mol and a pre-exponential factor of 1.2 × 10⁹. These findings support the hypothesis that reaction rates approximately double for each 10°C increase in temperature within the studied range. The high R² value of 0.9982 indicates an excellent fit to the Arrhenius model, validating its applicability for predicting reaction kinetics in similar systems.\n              \u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-section\"\u003e\n                \u003ch3\u003e1. Introduction\u003c/h3\u003e\n                \u003cp\u003eTemperature is one of the most critical factors affecting the rate of chemical reactions. The relationship between temperature and reaction rate has been extensively studied and is commonly described by the Arrhenius equation. This equation provides a mathematical model that relates the rate constant of a chemical reaction to the absolute temperature, activation energy, and pre-exponential factor.\u003c/p\u003e\n                \u003cp\u003eIn this study, we aim to experimentally verify the Arrhenius relationship and quantify the effect of temperature on reaction rates in a controlled system. Our hypothesis predicts that reaction rates will increase exponentially with temperature, approximately doubling for each 10°C increase.\u003c/p\u003e\n              \u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-section\"\u003e\n                \u003ch3\u003e2. Methodology\u003c/h3\u003e\n                \u003cp\u003eWe conducted a controlled experiment using a model reaction system across five temperature points (10°C, 20°C, 30°C, 40°C, and 50°C). All other variables, including reactant concentration, pressure, and catalyst presence, were held constant. Reaction rates were measured using spectrophotometry, with six replicate measurements at each temperature to ensure statistical validity.\u003c/p\u003e\n              \u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-section\"\u003e\n                \u003ch3\u003e3. Results\u003c/h3\u003e\n                \u003cp\u003eThe experimental data showed a clear exponential relationship between temperature and reaction rate. The measured rates increased from 0.0015 mol/L/s at 10°C to 0.0295 mol/L/s at 50°C. Fitting the data to the Arrhenius equation yielded an activation energy of 45.2 kJ/mol and a pre-exponential factor of 1.2 × 10⁹, with an R² value of 0.9982.\u003c/p\u003e\n              \u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-section\"\u003e\n                \u003ch3\u003e4. Discussion\u003c/h3\u003e\n                \u003cp\u003eOur results strongly support the hypothesis that reaction rates increase exponentially with temperature according to the Arrhenius equation. The high R² value indicates that the model explains nearly all of the variance in the observed data. The calculated activation energy is consistent with typical values for this type of reaction.\u003c/p\u003e\n                \u003cp\u003eLimitations of this study include the relatively narrow temperature range and the focus on a single reaction type. Future research should expand the temperature range and investigate different chemical systems to test the broader applicability of these findings.\u003c/p\u003e\n              \u003c/div\u003e\n              \n              \u003cdiv class\u003d\"paper-section\"\u003e\n                \u003ch3\u003e5. Conclusion\u003c/h3\u003e\n                \u003cp\u003eThis study confirms that temperature has a significant and predictable effect on reaction rates, following the Arrhenius equation. The experimental results provide quantitative parameters that can be used to predict reaction rates at different temperatures within the studied range. These findings have implications for optimizing reaction conditions in various chemical processes and understanding temperature-dependent phenomena in natural systems.\u003c/p\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"d-flex justify-content-between mt-4\"\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-outline-primary\" id\u003d\"prevStepBtn\"\u003ePrevious Step\u003c/button\u003e\n              \u003cbutton type\u003d\"button\" class\u003d\"btn btn-primary\" id\u003d\"finishBtn\"\u003eFinish \u0026 Return to Dashboard\u003c/button\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class\u003d\"col-lg-4\"\u003e\n        \u003cdiv class\u003d\"card\"\u003e\n          \u003cdiv class\u003d\"card-header\"\u003e\n            \u003ch5 class\u003d\"card-title mb-0\"\u003eExport Options\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv class\u003d\"card-body\"\u003e\n            \u003cdiv class\u003d\"export-option selected\" id\u003d\"pdfOption\"\u003e\n              \u003cdiv class\u003d\"export-icon\"\u003ePDF\u003c/div\u003e\n              \u003cdiv class\u003d\"export-details\"\u003e\n                \u003ch5\u003ePDF Document\u003c/h5\u003e\n                \u003cp\u003eStandard format for academic papers\u003c/p\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"export-option\" id\u003d\"docxOption\"\u003e\n              \u003cdiv class\u003d\"export-icon\"\u003eDOC\u003c/div\u003e\n              \u003cdiv class\u003d\"export-details\"\u003e\n                \u003ch5\u003eWord Document\u003c/h5\u003e\n                \u003cp\u003eEditable format for further modifications\u003c/p\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class\u003d\"export-option\" id\u003d\"htmlOption\"\u003e\n              \u003cdiv class\u003d\"export-icon\"\u003eHTML\u003c/div\u003e\n        