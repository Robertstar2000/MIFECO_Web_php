f user is authenticated\n    document.addEventListener(\u0027DOMContentLoaded\u0027, function() {\n      const isAuthenticated \u003d localStorage.getItem(\u0027isAuthenticated\u0027);\n      const userEmail \u003d localStorage.getItem(\u0027userEmail\u0027);\n      \n      if (!isAuthenticated || !userEmail) {\n        // Redirect to login if not authenticated\n        window.location.href \u003d \u0027../login.html\u0027;\n        return;\n      }\n      \n      // Display user email\n      document.getElementById(\u0027userEmail\u0027).textContent \u003d userEmail;\n      document.getElementById(\u0027paperAuthor\u0027).textContent \u003d userEmail;\n      \n      // Handle logout\n      document.getElementById(\u0027logoutBtn\u0027).addEventListener(\u0027click\u0027, function(e) {\n        e.preventDefault();\n        localStorage.removeItem(\u0027isAuthenticated\u0027);\n        localStorage.removeItem(\u0027userEmail\u0027);\n        window.location.href \u003d \u0027../login.html\u0027;\n      });\n      \n      // Handle export options\n      const exportOptions \u003d document.querySelectorAll(\u0027.export-option\u0027);\n      exportOptions.forEach(option \u003d\u003e {\n        option.addEventListener(\u0027click\u0027, function() {\n          // Remove selected class from all options\n          exportOptions.forEach(opt \u003d\u003e opt.classList.remove(\u0027selected\u0027));\n          // Add selected class to clicked option\n          this.classList.add(\u0027selected\u0027);\n        });\n      });\n      \n      // Handle export button\n      document.getElementById(\u0027exportBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Get selected export format\n        const selectedOption \u003d document.querySelector(\u0027.export-option.selected\u0027);\n        const format \u003d selectedOption.id.replace(\u0027Option\u0027, \u0027\u0027);\n        \n        // In a real app, this would generate and download the paper in the selected format\n        alert(`Your paper would be exported in ${format.toUpperCase()} format in the production version.`);\n      });\n      \n      // Handle previous step button\n      document.getElementById(\u0027prevStepBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        window.location.href \u003d \u0027peer-reviewer.html\u0027;\n      });\n      \n      // Handle finish button\n      document.getElementById(\u0027finishBtn\u0027).addEventListener(\u0027click\u0027, function() {\n        // Mark experiment as completed\n        localStorage.setItem(\u0027experimentCompleted\u0027, \u0027true\u0027);\n        // Navigate back to dashboard\n        window.location.href \u003d \u0027../dashboard.html\u0027;\n      });\n    });\n  \u003c/script\u003e\n\u003c/body\u003e\n\u003c/html\u003e",
